{
  "name": "ignorable",
  "version": "1.1.2",
  "description": "Detects whether a file in a package can be ignored",
  "keywords": [
    "ignore"
  ],
  "main": "index.js",
  "typings": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "safeIgnore",
    "unsafeIgnore"
  ],
  "scripts": {
    "build": "true",
    "md": "preview && mos",
    "pretest": "npm run build",
    "test": "preview && node test && mos t",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zkochan/ignorable.git"
  },
  "author": {
    "name": "Zoltan Kochan",
    "email": "z@kochan.io",
    "url": "https://www.kochan.io/",
    "twitter": "ZoltanKochan"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zkochan/ignorable/issues"
  },
  "homepage": "https://github.com/zkochan/ignorable#readme",
  "devDependencies": {
    "mos": "^2.0.0-alpha.3",
    "mos-plugin-readme": "^1.0.4",
    "package-preview": "^1.0.0",
    "tape": "^4.8.0"
  },
  "mos": {
    "plugins": [
      "readme"
    ],
    "installation": {
      "useShortAlias": true
    }
  },
  "dependencies": {
    "minimatch": "^3.0.4"
  }
}
